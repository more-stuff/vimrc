{
    "languageserver": {
        "go": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "trace.server": "verbose",
            "filetypes": ["go"]
        }
    },
    {
        "coc.preferences.formatOnSaveFiletypes": ["css", "Markdown"]
    },
    {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    }
}